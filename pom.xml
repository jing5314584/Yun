				<artifactId>docker-maven-plugin</artifactId>
				<version>0.4.13</version>
				<configuration>
					<imageName>yun</imageName>
					<dockerDirectory>src/main/resources</dockerDirectory>
					<dockerHost>http://101.132.161.206:6000</dockerHost>
					<resources>
